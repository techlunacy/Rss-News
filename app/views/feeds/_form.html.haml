= form_for @feed do |f|
  -if @feed.errors.any?
    #error_explanation
      %h2= "#{pluralize(@feed.errors.count, "error")} prohibited this feed from being saved:"
      %ul
        - @feed.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :title
    = f.text_field :title
  .field
    = f.label :url
    = f.text_field :url
  .field
    = f.label :feed_url
    = f.text_field :feed_url
    = f.submit 'get feed details'
  .field
    = f.label :etag
    = f.text_field :etag
  .field
    = f.label :last_modified
    = f.datetime_select :last_modified
  .field
    = f.label :is_valid
    = f.check_box :is_valid
  .actions
    = f.submit 'Save'
